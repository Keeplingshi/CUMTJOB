    <script>
    window.onload = function(){
		var oTable = document.getElementById('tab');
		var aATable = oTable.getElementsByTagName('a');

		for(var i=0;i<aATable.length;i++) {
			
		    aATable[i].onclick = function(){
				//==================================
				console.log(this.innerHTML);//专业
				var college='';
				if(this.parentNode.parentNode.getElementsByTagName('th')[0]){
					console.log(this.parentNode.parentNode.getElementsByTagName('th')[0].innerHTML);//学院
					college=this.parentNode.parentNode.getElementsByTagName('th')[0].innerHTML;
				}else {
					console.log(this.parentNode.parentNode.previousSibling.previousSibling.children[0].innerHTML);//学院
					college=this.parentNode.parentNode.previousSibling.previousSibling.children[0].innerHTML;
				}
				
				var major=this.innerHTML;
				
				window.location.href = "recommend.jsp?college=" + college+"&major="+major;
				
				//==================================
			}
			
		}
	}

